# Backend environment variables example
# Database (PostgreSQL / Neon)
DATABASE_URL=jdbc:postgresql://localhost:5432/yourdb
DATABASE_USERNAME=youruser
DATABASE_PASSWORD=yourpass

# Auth0 Resource Server (API)
AUTH0_ISSUER_URI=https://YOUR_DOMAIN.us.auth0.com/
AUTH0_AUDIENCE=https://your-api-identifier

# CORS origin for the frontend
FRONTEND_ORIGIN=http://localhost:3000
